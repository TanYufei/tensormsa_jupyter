   "source": [
    "import numpy as np\n",
    "dict_list = ['가','나','다','라','마','바','사','아','차']\n",
    "bucket = np.zeros(len(dict_list), dtype=np.float)\n",
    "\n",
    "for word in dict_list :\n",
    "    bucket_temp = bucket.copy()\n",
    "    np.put(bucket_temp, dict_list.index(word), 1)\n",
    "    print(bucket_temp)"
   ]
